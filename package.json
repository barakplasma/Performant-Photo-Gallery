{
  "name": "performant-photo-gallery",
  "version": "0.1.0",
  "homepage": "https://barakplasma.github.io/Performant-Photo-Gallery",
  "private": true,
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "vanilla-lazyload": "^8.7.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && react-snapshot",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": ["prettier --single-quote --write", "git add"]
  },
  "devDependencies": {
    "react-snapshot": "^1.3.0",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.3",
    "prettier": "^1.13.3",
    "react-scripts": "1.1.4"
  }
}
